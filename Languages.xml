<?xml version="1.0" encoding="UTF-8"?>

<settings>
	
	<language id="text.html.markdown">
		<root-zone>text.markdown</root-zone>
		<name>Markdown</name>
		<detectors>
			<extension>markdown</extension>
			<extension>markdn</extension>
			<extension>mkdn</extension>
			<extension>md</extension>
		</detectors>
	</language>
	
	<meta>
		<name>Markdown</name>
		<description>Syntax highlighting for John Gruber's Markdown</description>
		<author>David Czihak, Ian Beck</author>
		<identifier>at.davidczihak.Sugar.Markdown</identifier>
		<version status="preview">0.3.3</version>
		<download format="git">git://github.com/dc/markdown.sugar.git</download>
	</meta>
	
</settings>